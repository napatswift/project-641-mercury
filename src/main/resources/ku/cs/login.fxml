<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.LoginController">
   <Text fx:id="appNapeText" layoutX="295.0" layoutY="167.0" styleClass="h1" text="App Name" />
    <Text fx:id="loginText" styleClass="h3" text="Login" />
    <TextField fx:id="usernameTF" />
    <TextField fx:id="passwordTF" />
    <HBox alignment="BOTTOM_RIGHT" spacing="5.0">
        <Button fx:id="signUpBtn" style="-fx-background-color: alpha;" text="sign-up" />
        <Button fx:id="logInBtn" text="log-in" />
    </HBox>
   <padding>
      <Insets left="30.0" right="30.0" />
   </padding>
</VBox>
