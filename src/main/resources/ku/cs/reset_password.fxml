      <TextField fx:id="oldPasswordTextField" layoutX="104.0" layoutY="196.0" prefHeight="26.0" prefWidth="286.0" promptText="Old password" />
      <TextField fx:id="newPasswordTextField" layoutX="104.0" layoutY="278.0" prefHeight="26.0" prefWidth="286.0" promptText="New password" />
      <TextField fx:id="confirmNewPasswordTextField" layoutX="104.0" layoutY="368.0" prefHeight="26.0" prefWidth="286.0" promptText="Confirm new password" />
      <Label layoutX="104.0" layoutY="179.0" text="Old password" />
      <Label layoutX="104.0" layoutY="261.0" text="New password" />
      <Label layoutX="104.0" layoutY="351.0" text="Confirm new password" />
      <Label fx:id="userName" layoutX="34.0" layoutY="97.0" prefHeight="59.0" prefWidth="206.0" text="userName">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <ImageView fx:id="userImageView" fitHeight="150.0" fitWidth="200.0" layoutX="247.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="name" layoutX="34.0" layoutY="37.0" prefHeight="59.0" prefWidth="206.0" text="name">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <Label fx:id="description" layoutX="104.0" layoutY="429.0" prefHeight="26.0" prefWidth="286.0" textAlignment="CENTER" />
   </children>
</AnchorPane>
